{
  "name": "cliw",
  "description": "A cliw[rapper] to ease day to day command line tasks!",
  "version": "0.1.0",
  "author": "kgalli @kgalli",
  "bin": {
    "cliw": "./bin/run"
  },
  "bugs": "https://github.com/kgalli/cliw/issues",
  "dependencies": {
    "@oclif/command": "^1.5.18",
    "@oclif/config": "^1.13.2",
    "@types/ip": "^1.1.0",
    "aws-sdk": "^2.499.0",
    "chalk": "^2.4.2",
    "cli-ux": "^5.3.1",
    "inquirer": "^6.5.0",
    "ip": "^1.1.5",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/plugin-help": "^2.2.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/chai": "^4.1.7",
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "0.0.43",
    "@types/js-yaml": "^3.12.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.13",
    "chai": "^4.2.0",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kgalli/cliw",
  "keywords": [
    "oclif, cli, database tools, docker compose"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cliw",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "secret": {
        "description": "encrypt, decrypt secret via AWS KMS"
      },
      "db": {
        "description": "dump, restore or run a database console"
      },
      "service": {
        "description": "manage services to run as docker containers"
      },
      "config": {
        "description": "get and set project config properties"
      }
    }
  },
  "repository": "kgalli/cliw",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
